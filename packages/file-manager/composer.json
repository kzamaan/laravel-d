{
    "name": "draftscripts/file-manager",
    "description": "Laravel File Manager",
    "license": "MIT",
    "authors": [
        {
            "name": "KAMRUZZAMAN",
            "email": "kzamanbn@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0",
        "league/flysystem": "^3.28",
        "league/flysystem-ftp": "^3.28",
        "league/flysystem-sftp-v3": "^3.28"
    },
    "autoload": {
        "psr-4": {
            "DraftScripts\\FileManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DraftScripts\\FileManager\\FileManagerServiceProvider"
            ],
            "aliases": {
                "FileManager": "DraftScripts\\FileManager\\Facades\\FileManager"
            }
        }
    },
    "minimum-stability": "stable"
}
